@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <div class="row">
        <div class="form-group col-6 mx-auto">
            <label for="First Name" class="control-label"></label>
            <input name="test" class="form-control blinking" placeholder="Enter First Name" />
        </div>
    </div>
    <div class="row">
        <div class="form-group col-6 mx-auto">
            <label for="Last Name" class="control-label"></label>
            <input name="test2" class="form-control" placeholder="Enter Last Name" />
        </div>
    </div>
    <div class="row">
        <div class="form-group col-6 mx-auto">
            <label for="Company Name" class="control-label"></label>
            <input name="test3" class="form-control" placeholder="Enter Company Name" />
        </div>
    </div>
    <div class="row">
        <div class="form-group col-6 mx-auto">
            <label for="Phone Number" class="control-label"></label>
            <input name="test4" class="form-control" placeholder="Enter Phone Number" />
        </div>
    </div>
    <div class="row">
        <div class="col-6 mx-auto">
            <button id="pre" class="btn btn-warning mr-1 float-left" type="button">Previous</button>
            <button id="nxt" class="btn btn-primary ml-1 float-right" type="button">Next</button>
        </div>
    </div>
</div>
